{
  "name": "laa-dashboard",
  "version": "2.0.0",
  "private": true,
  "license": "MIT",
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "postcss-normalize": "^10.0.1",
    "react-scripts": "^5.0.1"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.186.0",
    "@aws-sdk/lib-dynamodb": "^3.186.0",
    "aws-amplify": "^4.3.37",
    "font-awesome": "^4.7.0",
    "react": "^18.2.0",
    "react-app-polyfill": "^3.0.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.2"
  },
  "engines": {
    "node": "^16.17.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js"
    ]
  },
  "scripts": {
    "start": "sh -ac '. .env.${REACT_APP_ENV} && react-scripts start'",
    "build": "sh -ac '. .env.${REACT_APP_ENV} && react-scripts build'",
    "build:test": "REACT_APP_ENV=test yarn run build",
    "build:prod-int": "REACT_APP_ENV=prod-int yarn run build",
    "build:prod-ext": "REACT_APP_ENV=prod-ext yarn run build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint .",
    "lint:fix": "eslint --fix --ext .js,.jsx .",
    "audit": "yarn audit --groups dependencies"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
